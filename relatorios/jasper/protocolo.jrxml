	<queryString>
		<![CDATA[select
	cli.nome_completo as nome_cliente,
	pc.nome_completo as nome_parte_contraria,
	numero_protocolo_id,
	td.nome as tipo_demanda,
	ts.nome as tipo_status,
	date_format(ft.created_at,'%d/%m/%Y às %Hh%i') as created_at,
	EXTRACT(MONTH FROM ft.created_at) as mes
from
	ficha_triagens ft
inner join clientes cli ON
	cli.id = ft.cliente_id
inner join clientes pc ON
	pc.id = ft.parte_contraria_id
inner join tipo_demandas td ON
	td.id = ft.tipo_demanda_id
inner join tipo_status ts ON
	ts.id = ft.tipo_status_id
where
	ft.id =  $P{id}]]>
	</queryString>
